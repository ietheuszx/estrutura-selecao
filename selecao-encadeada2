let mes = 10;
if (mes === 1 || mes === 3 || mes === 5 || mes === 7 || mes === 8 || mes === 10 || mes === 12) {
    console.log("Este mes tem mais dias de trabalhoKKKKKKKKKKKKKKK")
} else if (mes ===4 || mes === 6 || mes === 9 || mes === 11) {
    console.log("Que pena, um dia a menos para trabaia :(")
} else if (mes === 2) {
    console.log("Este é o mês menos produtivo")
} else {
    console.log("Ainda não tem como trabaia mais que 12 meses meu guri... ainda")
}